generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  email      String
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@unique([email], map: "emailShouldBeUnique")
}
